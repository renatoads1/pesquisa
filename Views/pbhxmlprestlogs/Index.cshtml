@model IEnumerable<pesquisa.Models.ViewModels.pbhxmlprestlog>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.empresa)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.cnpj)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.mes_de_vigencia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.sucesso)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.descricao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.quantidade_de_nfs_baixadas)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.quantidade_de_nfs_processadas)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.tempo_execucao)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.empresa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.cnpj)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.mes_de_vigencia)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.sucesso)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.descricao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.quantidade_de_nfs_baixadas)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.quantidade_de_nfs_processadas)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.tempo_execucao)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
